ScrollHandler();\n        return;\n      }\n      setTimeout(() => {\n        this.scrollParent = document.querySelector('.page-component__scroll > .el-scrollbar__wrap');\n        this.scrollParent && this.scrollParent.addEventListener('scroll', this.scrollHandler);\n        this.scrollHandler();\n      }, 200);\n    }\n  },\n  mounted () {\n    this.$nextTick(() => {\n      let highlight = this.$el.getElementsByClassName('highlight')[0];\n      if (this.$el.getElementsByClassName('description').length === 0) {\n        highlight.style.width = '100%';\n        highlight.borderRight = 'none';\n      }\n    });\n  },\n  beforeDestroy () {\n    this.removeScrollHandler();\n  }\n};\n"],"mappings":"AACA,eAAe;EACbA,IAAI,GAAI;IACN,OAAO;MACLC,QAAQ,EAAE,KADL;MAELC,UAAU,EAAE,KAFP;MAGLC,YAAY,EAAE,KAHT;MAILC,YAAY,EAAE,IAJT;MAKLC,UAAU,EAAE;QACV,aAAa,MADH;QAEV,aAAa,MAFH;QAGV,eAAe,MAHL;QAIV,gBAAgB;MAJN;IALP,CAAP;EAYD,CAdY;;EAebC,OAAO,GAAE;IACPC,OAAO,CAACC,GAAR,CAAY,CAAZ;IACAD,OAAO,CAACC,GAAR,CAAY,KAAKC,MAAjB;EACD,CAlBY;;EAmBbC,KAAK,EAAE;IACLC,QAAQ,EAAEC,MADL;;IAELC,OAAO,GAAI;MACT,OAAO,EAAP;IACD;;EAJI,CAnBM;EAyBbC,OAAO,EAAE;IACPC,aAAa,GAAI;MACf,MAAM;QAAEC,GAAF;QAAOC,MAAP;QAAeC;MAAf,IAAwB,KAAKC,KAAL,CAAWC,IAAX,CAAgBC,qBAAhB,EAA9B;MACA,KAAKlB,YAAL,GAAoBc,MAAM,GAAGK,QAAQ,CAACC,eAAT,CAAyBC,YAAlC,IAClBR,GAAG,GAAG,EAAN,IAAYM,QAAQ,CAACC,eAAT,CAAyBC,YADvC;IAED,CALM;;IAMPC,mBAAmB,GAAI;MACrB,KAAKrB,YAAL,IAAqB,KAAKA,YAAL,CAAkBsB,mBAAlB,CAAsC,QAAtC,EAAgD,KAAKX,aAArD,CAArB;IACD;;EARM,CAzBI;EAmCbY,QAAQ,EAAE;IACRC,IAAI,GAAI;MACN,OAAO,KAAKC,MAAL,CAAYC,IAAZ,CAAiBC,KAAjB,CAAuB,GAAvB,EAA4B,CAA5B,CAAP;IACD,CAHO;;IAIRC,UAAU,GAAI;MACZ,OAAQ,QAAO,KAAKJ,IAAK,SAAQ,KAAKK,OAAL,CAAaC,YAAb,CAA0BJ,IAA1B,CAA+BC,KAA/B,CAAqC,GAArC,EAA0CI,GAA1C,EAAgD,EAAjF;IACD,CANO;;IAORC,SAAS,GAAI;MACX,OAAO,KAAKlC,UAAL,GAAkB,mBAAlB,GAAwC,sBAA/C;IACD,CATO;;IAURmC,WAAW,GAAI;MACb,OAAO,KAAKnC,UAAL,GAAkB,KAAKG,UAAL,CAAgB,WAAhB,CAAlB,GAAiD,KAAKA,UAAL,CAAgB,WAAhB,CAAxD;IACD,CAZO;;IAaRiC,QAAQ,GAAI;MACV,OAAO,KAAKC,GAAL,CAASC,sBAAT,CAAgC,MA